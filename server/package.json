{
  "name": "example-node-server",
  "version": "1.0.0",
  "description": "SPS server",
  "main": "lib/index.js",
  "scripts": {
    "start": "nodemon lib/index.js --exec babel-node --extensions '.ts,.js' --presets es2015,stage-2",
    "debug": "nodemon --inspect lib/index.js --exec babel-node --extensions '.ts,.js' --presets es2015,stage-2",
    "build": "babel --extensions '.ts,.js' lib -d dist",
    "db:create": "node_modules/.bin/create_database",
    "db:drop": "node_modules/.bin/drop_database",
    "db:migrate": "node_modules/.bin/sequelize db:migrate",
    "db:seed": "node_modules/.bin/sequelize db:seed:all",
    "db:reset": "npm run db:drop; npm run db:create; npm run db:migrate",
    "serve": "node ./dist/index.js",
    "test": "mocha --compilers js:babel-register"
  },
  "author": "Ozzie Gooen",
  "license": "MIT",
  "dependencies": {
    "apollo-client": "^1.9.3",
    "apollo-client-preset": "0.1.0-1",
    "apollo-server-express": "^1.1.7",
    "async": "^2.6.0",
    "babel-cli": "^6.26.0",
    "babel-preset-stage-3": "^6.24.1",
    "body-parser": "^1.18.2",
    "case": "^1.5.4",
    "cookie-parser": "~1.4.3",
    "cors": "^2.8.4",
    "debug": "~2.6.9",
    "dotenv": "^4.0.0",
    "es6-promise": "^4.1.1",
    "express": "^4.16.2",
    "express-graphql": "^0.6.11",
    "graphql": "^0.11.7",
    "graphql-client": "^2.0.0",
    "graphql-relay": "^0.5.4",
    "graphql-sequelize": "^5.6.1",
    "graphql-tag": "^2.5.0",
    "graphql-tools": "^2.5.1",
    "lodash": "^4.17.4",
    "pg": "^7.3.0",
    "pg-hstore": "^2.3.2",
    "pluralize": "^7.0.0",
    "sequelize": "^4.23.3",
    "sequelize-cli": "^3.2.0",
    "sequelize-migration-pg-extras": "^1.0.1"
  },
  "devDependencies": {
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-register": "^6.11.6",
    "mocha": "^3.0.1",
    "nodemon": "^1.12.1",
    "rimraf": "^2.6.2"
  }
}
