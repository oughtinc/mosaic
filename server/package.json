{
  "name": "example-node-server",
  "version": "1.0.0",
  "description": "SPS server",
  "main": "lib/index.js",
  "scripts": {
    "start": "nodemon lib/index.js --exec ts-node --extensions '.ts,.js' --presets es2015,stage-2",
    "debug": "nodemon --inspect lib/index.js --exec ts-node --extensions '.ts,.js' --presets es2015,stage-2",
    "build": "babel --extensions '.ts,.js' lib -d dist",
    "db:create": "node_modules/.bin/create_database",
    "db:drop": "node_modules/.bin/drop_database",
    "db:migrate": "node_modules/.bin/sequelize db:migrate",
    "db:migrate:undo": "node_modules/.bin/db:migrate:undo",
    "db:seed": "node_modules/.bin/sequelize db:seed:all",
    "db:reset": "npm run db:drop; npm run db:create; npm run db:migrate",
    "serve": "node ./dist/index.js",
    "test": "mocha --compilers js:babel-register"
  },
  "author": "Ozzie Gooen",
  "license": "MIT",
  "dependencies": {
    "apollo-server-express": "^1.1.7",
    "async": "^2.6.0",
    "body-parser": "^1.18.2",
    "case": "^1.5.4",
    "cookie-parser": "~1.4.3",
    "cors": "^2.8.4",
    "debug": "~2.6.9",
    "dotenv": "^5.0.1",
    "es6-promise": "^4.1.1",
    "express": "^4.16.3",
    "express-graphql": "^0.6.11",
    "graphql": "^0.13.1",
    "graphql-client": "^2.0.0",
    "graphql-relay": "^0.5.4",
    "graphql-sequelize": "^8.0.0",
    "graphql-tag": "^2.5.0",
    "graphql-tools": "^2.22.0",
    "graphql-type-json": "^0.2.0",
    "lodash": "^4.17.4",
    "pg": "^7.3.0",
    "pg-hstore": "^2.3.2",
    "pluralize": "^7.0.0",
    "reflect-metadata": "^0.1.12",
    "sequelize": "^4.36.1",
    "sequelize-cli": "^4.0.0",
    "sequelize-migration-pg-extras": "^1.0.1",
    "sequelize-typescript": "^0.6.3",
    "slate": "^0.33.0"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.40",
    "@babel/node": "^7.0.0-beta.40",
    "@babel/plugin-proposal-decorators": "^7.0.0-beta.40",
    "@babel/plugin-transform-typescript": "^7.0.0-beta.40",
    "babel-cli": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-3": "^6.24.1",
    "babel-preset-typescript": "^7.0.0-alpha.19",
    "babel-register": "^6.11.6",
    "immutable": "^3.8.2",
    "mocha": "^3.0.1",
    "nodemon": "^1.12.1",
    "rimraf": "^2.6.2",
    "ts-node": "^5.0.1",
    "typescript": "^2.7.2"
  }
}
