{
    "extends": [
      "tslint-react"
    ],
    "rules": {
      "no-default-export": true,
      "adjacent-overload-signatures": true,
      "no-var-requires": true,
      "prefer-const": false,
      "await-promise": true,
      "restrict-plus-operands": true,
      "deprecation": true,
      "arrow-parens": true,
      "callable-types": true,
      "no-irregular-whitespace": true,
      "no-unused-variable": true,
      "object-literal-key-quotes": [
        true,
        "as-needed"
      ],
      "object-literal-shorthand": false,
      "max-classes-per-file": [
        true,
        3
      ],
      "member-access": [
        true,
        "check-accessor",
        "check-constructor"
      ],
      "align": [
        true,
        "parameters",
        "arguments",
        "statements"
      ],
      "ban": false,
      "class-name": true,
      "comment-format": [
        true,
        "check-space"
      ],
      "curly": true,
      "eofline": false,
      "forin": true,
      "indent": [
        true,
        "spaces",
        2
      ],
      "interface-name": [
        true,
        "never-prefix"
      ],
      "jsdoc-format": true,
      "jsx-no-lambda": false,
      "jsx-no-multiline-js": false,
      "label-position": true,
      "max-line-length": [
        false,
        120
      ],
      "member-ordering": [
        true,
        {
          "order": "fields-first"
        }
      ],
      "no-any": false,
      "no-arg": true,
      "no-bitwise": true,
      "no-console": [
        false,
        "log",
        "error",
        "debug",
        "info",
        "time",
        "timeEnd",
        "trace"
      ],
      "no-consecutive-blank-lines": [
        true
      ],
      "no-construct": true,
      "no-debugger": true,
      "no-duplicate-super": true,
      "no-duplicate-variable": true,
      "no-empty": true,
      "no-eval": true,
      "no-floating-promises": true,
      "no-shadowed-variable": false,
      "no-invalid-template-strings": true,
      "no-misused-new": true,
      "no-string-throw": true,
      "no-string-literal": true,
      "no-switch-case-fall-through": true,
      "no-trailing-whitespace": false,
      "no-var-keyword": true,
      "no-unused-expression": true,
      "no-use-before-declare": true,
      "one-line": [
        true,
        "check-catch",
        "check-else",
        "check-open-brace",
        "check-whitespace"
      ],
      "quotemark": [
        true,
        "double",
        "jsx-double"
      ],
      "radix": true,
      "semicolon": [
        true,
        "always"
      ],
      "switch-default": true,
      "trailing-comma": [
        true,
        {
          "multiline": {
            "objects": "always",
            "arrays": "always",
            "functions": "ignore",
            "typeLiterals": "ignore"
          },
          "singleline": "never"
        }
      ],
      "triple-equals": [
        true,
        "allow-null-check"
      ],
      "typedef": [
        true,
        "parameter",
        "property-declaration"
      ],
      "typedef-whitespace": [
        true,
        {
          "call-signature": "nospace",
          "index-signature": "nospace",
          "parameter": "nospace",
          "property-declaration": "nospace",
          "variable-declaration": "nospace"
        }
      ],
      "variable-name": [
        true,
        "ban-keywords",
        "check-format",
        "allow-leading-underscore",
        "allow-pascal-case"
      ],
      "whitespace": [
        true,
        "check-branch",
        "check-decl",
        "check-module",
        "check-operator",
        "check-separator",
        "check-type",
        "check-typecast"
      ]
    }
  }
  